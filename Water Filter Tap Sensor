// IDEATED PROJECT

#include<stdio.h>
int main(){
    int x;
    printf("Currently tap is off\n");
    printf("Enter your value of a \n");
    scanf("%d", &x);

    switch(x){
        case 1 :
        printf("Currently tap is on and water comes out of tap\n");
        break;

        case 0:
        printf("Currently tap is off and water doesn't come out off tap\n");
        break;

        default:
        printf("Sensor Error!\n");
    }
    return 0;
}
   
